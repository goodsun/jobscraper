        "working_hours": "dl.p-detailInfoList dt:contains('勤務時間') + dd",
        "holiday": "dl.p-detailInfoList dt:contains('休日') + dd",
        "welfare_program": "dl.p-detailInfoList dt:contains('福利厚生') + dd",
