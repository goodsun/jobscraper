{
    "name": "benesse-mcm",
    "domain": "kango.benesse-mcm.jp",
    "selectors": {
        "name": "h1.title",
        "price": ".list01 dt.wages + dd, .m_jobInfoTable02 th.salary + td",
        "area": ".list01 dt.spot + dd, .m_jobInfoTable02 th.spot + td",
        "access": ".m_jobInfoTable02 th.train + td",
        "occupation": ".m_jobInfoTable01 th:contains('募集職種') + td",
        "contract": ".m_jobInfoTable01 th:contains('雇用形態') + td",
        "detail": ".m_jobInfoTable01 th:contains('仕事内容') + td",
        "required_skill": ".m_jobInfoTable02 th:contains('必要なスキル') + td",
        "holiday": ".m_jobInfoTable02 th:contains('休日・休暇') + td",
        "working_hours": ".m_jobInfoTable02 th.jobstyle + td",
        "working_style": ".m_jobInfoTable02 th.kinmu_nissu + td",
        "welfare_program": ".m_jobInfoTable02 th:contains('福利厚生') + td",
        "license": ".m_jobInfoTable02 th.shikaku + td",
        "facility_type": ".list01 dt.home + dd",
        "dept": ".m_jobInfoTable02 th:contains('診療科目') + td",
        "position": ".m_jobInfoTable02 th:contains('配属先') + td",
        "station": ".m_jobInfoTable02 th.train + td"
    },
    "patterns": {
        "facility_name": "^([^／]+)／",
        "salary_hourly": "時給[：:]\\s*([0-9,]+)円",
        "salary_monthly": "月収[：:]\\s*([0-9,]+)円",
        "prefecture": "^([^都道府県]+[都道府県])",
        "city": "[都道府県]([^市区町村]+[市区町村])"
    }
}