{
    "name": "kango-oshigoto",
    "domain": "kango-oshigoto.jp",
    "selectors": {
        "area": "h3:contains('勤務地') ~ p",
        "station": "h3:contains('交通情報') ~ p"
    },
    "patterns": {
        "salary_monthly": "月収\\s*([0-9,]+)円",
        "prefecture": "^([^都道府県]+[都道府県])",
        "city": "[都道府県]([^市区町村]+[市区町村])"
    }
}